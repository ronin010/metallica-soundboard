{
  "expo": {
    "name": "metallica-soundboard",
    "slug": "metallica-soundboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/metallica-logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
